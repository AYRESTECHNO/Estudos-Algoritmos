algoritmo "SomaFuncao"
// Função :
// Autor :
// Data : 02/03/2019
// Seção de Declarações
var
   N1, N2, S: Inteiro
   
Funcao Soma(A, B: Inteiro): Inteiro
Inicio

      Retorne A + B

FimFuncao

inicio
// Seção de Comandos
   N1 <- 5
   N2 <- 7
   S<- Soma(N1, N2)
   Escreval("A soma entre ", N1,  " e ", N2, " é igual a: ", S)


fimalgoritmo
