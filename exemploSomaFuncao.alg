algoritmo "exemploSomaFuncao"
// Função :
// Autor :
// Data : 11/04/2020
// Seção de Declarações 
var
   N1, N2, RES: Inteiro
Funcao Soma(A, B: Inteiro): Inteiro

var
   S: Inteiro
inicio
      S := A + B
      Retorne S
FimFuncao
inicio
// Seção de Comandos 
   N1 := 5
   N2 := 4
   RES := Soma(N1, N2)
   Escreval("A Soma é =", RES)


fimalgoritmo
