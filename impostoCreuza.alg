algoritmo "impostoCreuza"
// Função :
// Autor :
// Data : 05/04/2020
// Seção de Declarações 
var
   Preco, Imposto: Real
inicio
// Seção de Comandos
   Escreva("Qual o Valor do Produto? US$ ")
   Leia(Preco)
   Imposto := ((Preco * 60)/100)
   Escreval("O Valor do Imposto Será US$ ", Imposto:5:2)

 
fimalgoritmo
