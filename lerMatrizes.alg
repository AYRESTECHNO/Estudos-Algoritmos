algoritmo "lerMatrizes"
// Função :
// Autor :
// Data : 07/03/2019
// Seção de Declarações 
var
   mat: vetor[1..3, 1..2] de Inteiro
   l, c: Inteiro
inicio
// Seção de Comandos
   Para l <- 1 ate 3 faca
        Para c <- 1 ate 2 faca
             Escreva("Digite o valor da posição [", l, ", ", c, "]: ")
             Leia(mat[l,c])
        Fimpara
   Fimpara


   Para l <- 1 ate 3 faca
        Para c <- 1 ate 2 faca
             Escreva(mat[l,c]:5)
        Fimpara
        Escreval()
   Fimpara
fimalgoritmo
